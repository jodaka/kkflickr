{
  "manifest_version": 2,

  "name": "KKF flickr uploader",
  "description": "KKF",
  "version": "1.0",
  "permissions": [
    "http://*.kitchenknifeforums.com/*",
    "http://*.flickr.com/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [ "http://*.kitchenknifeforums.com/*" ],
      "js": [ "md5.js", "flickr.js" ],
      "css": [ "kkflickr.css" ]
      }
  ],
  "options_page": "options.html"
}